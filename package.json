    "git-commit-msg-linter": "^4.1.3",
    "husky": "^8.0.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^28.1.1",
    "jest-environment-jsdom": "^28.1.1",
    "jest-localstorage-mock": "^2.4.21",
    "lint-staged": "^13.0.2",
    "node-sass": "^7.0.1",
    "prettier": "^2.7.1",
    "sass-loader": "^13.0.0",
    "style-loader": "^3.3.1",
    "ts-jest": "^28.0.5",
